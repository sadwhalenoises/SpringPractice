<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"> <!-- bean definitions here -->
        <bean id="Phone" class="org.example.Phone">
                <property name="number" value="000-0000"/>
        </bean>
        <bean id="Phone1" class="org.example.Phone">
                <property name="number" value="111-1111"/>
        </bean>
        <bean id="Phone2" class="org.example.Phone">
                <property name="number" value="222-2222"/>
        </bean>

        <context:component-scan base-package="org.example"/>
</beans>